<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.gob.cordoba.gobiernoabierto.agendacultural" version="1.2.155" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Agenda Cultural de Córdoba</name>
    <description>
        Agenda completa de eventos culturales de la Ciudad de Córdoba
    </description>
    <author email="gobiernoabiertocba@gmail.com" href="https://gobiernoabierto.cordoba.gob.ar/">
        Secretaría de Modernización de la Ciudad de Córdoba
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon density="hdpi" src="www/res/icono-64x64.png" />
        <splash density="mdpi" height="202" src="www/res/splash.png" width="202" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#FFFFFF" />
        <preference name="StatusBarStyle" value="default" />
        <gap:plugin name="org.apache.cordova.inappbrowser" />
    </platform>
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <engine name="android" spec="~6.2.2" />
    <engine name="ios" spec="~4.5.4" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.2" />
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.1.3" />
    <plugin name="cordova-plugin-statusbar" spec="~2.3.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
    <plugin name="cordova-plugin-calendar" spec="^5.0.0">
        <variable name="CALENDAR_USAGE_DESCRIPTION" value=" " />
    </plugin>
</widget>
